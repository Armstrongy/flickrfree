<?xml version="1.0" encoding="utf-8"?>

<AbsoluteLayout
	android:id="@+id/AbsoluteLayout01"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	
	<EditText
		android:id="@+id/authnum1"
		android:layout_height="wrap_content"
		android:layout_x="60dip"
		android:layout_y="100dip"
		android:layout_width="60dip"
		android:text="">
	</EditText>
  		
	<EditText
		android:id="@+id/authnum2"
		android:layout_height="wrap_content"
		android:layout_x="130dip"
		android:layout_y="100dip"
		android:layout_width="60dip"
		android:text="">
	</EditText>
  		
	<EditText
		android:id="@+id/authnum3"
		android:layout_height="wrap_content"
		android:layout_x="200dip"
		android:layout_y="100dip"
		android:layout_width="60dip"
		android:text="">
	</EditText>
  		
	<Button
		android:layout_y="187dip"
		android:layout_height="wrap_content"
		android:layout_width="90dip"
		android:layout_x="50dip"
		android:id="@+id/btnGetCode"
		android:text="Get Code">
	</Button>

	<Button
		android:layout_y="187dip"
		android:layout_height="wrap_content"
		android:layout_width="120dip"
		android:layout_x="150dip"
		android:id="@+id/btnAuthenticate"
		android:text="Authenticate">
	</Button>

	
	
	
	
</AbsoluteLayout>
